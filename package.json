{
    "name": "portfolio",
    "version": "1.0.0",
    "description": "Raphael Ehindero's portfolio",
    "scripts": {
      "start:server": "npm run start --prefix server",
      "start:frontend": "npm run start --prefix frontend",
      "start": "npm run start:server && npm run start:frontend",
      "build": "npm run build --prefix frontend && npm run start --prefix server",

      "dev:server": "yarn --cwd server develop",
      "dev:frontend": "wait-on http://localhost:1337/admin && yarn --cwd frontend dev",
      "dev": "cross-env FORCE_COLOR=1 npm-run-all -l -p dev:*"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
      "npm-run-all": "4.1.5",
      "wait-on": "5.2.1",
      "cross-env": "7.0.3"
    }
  }
  